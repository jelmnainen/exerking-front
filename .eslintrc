{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "react/jsx-indent": [2, 2],
    "react/no-deprecated": 2,
    "react/prop-types": 1,
    "react/prefer-stateless-function": 0,
    "new-cap": 0,
    "quote-props": [2, "consistent-as-needed"],
    "no-console": 0,
    "indent": [2, 2]
  }
}
